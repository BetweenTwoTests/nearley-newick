{
  "name": "nearley-newick",
  "version": "0.0.1",
  "description": "Newick tree format parser using nearley.js",
  "main": "index.js",
  "scripts": {
    "gen-parser": "nearleyc $GRAMMAR/$GRAMMAR.ne -o $GRAMMAR/$GRAMMAR.js",
    "run-parser": "node parse.js $GRAMMAR",
    "parse": "nearleyc grammars/$GRAMMAR/$GRAMMAR.ne -o grammars/$GRAMMAR/$GRAMMAR.js && node parse.js $GRAMMAR",
    "test": "npm run parse tests/ex-0-no-nodes-named.tree,tests/ex-1-leaf-nodes-named.tree,tests/ex-2-all-nodes-named.tree,tests/ex-3-all-but-root-have-distance-to-parent.tree,tests/ex-4-all-have-distance-to-parent.tree,tests/ex-5-distances-and-leaf-names.tree,tests/ex-6-distances-and-all-names.tree,tests/ex-7-tree-rooted-on-leaf-node.tree"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BetweenTwoTests/nearley-newick.git"
  },
  "keywords": [
    "newick",
    "nearley",
    "phylogenetics",
    "tree"
  ],
  "author": "BetweenTwoTests",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BetweenTwoTests/nearley-newick/issues"
  },
  "homepage": "https://github.com/BetweenTwoTests/nearley-newick#readme",
  "dependencies": {
    "aguid": "^2.0.0",
    "nearley": "^2.20.1"
  }
}
